  dialogueLines:
  - content: "\u5443\u2026\u2026"
    isPlayer: 1
  - content: "\u90A3\u6761\u9634\u5F71\u4E2D\u7684\u201C\u730E\u72AC\u201D\u6B64\u523B\u5C31\u5728\u4E0D\u8FDC\u5904\u5486\u54EE\u7740\uFF0C\u6211\u7684\u7075\u9B42\u5982\u5782\u6B7B\u7684\u730E\u7269\u56DE\u5E94\u7740\u5B83\u8D85\u8D8A\u65F6\u95F4\u4E0E\u7A7A\u95F4\u7684\u5636\u543C\u3002"
    isPlayer: 1
  - content: "\u5582\u5582~\u518D\u770B\u5B83\u4E5F\u4E0D\u4F1A\u8DEA\u4E0B\u5411\u4F60\u5410\u820C\u5934\u7684\u54E6~\r\n\u6211\u62AC\u8D77\u5934\uFF0C\u770B\u5411\u58F0\u97F3\u4F20\u6765\u7684\u65B9\u5411\u2014\u2014\u4E00\u4E2A\u7CBE\u81F4\u7684\u5C0F\u8EAB\u5F71\u4ECE\u5E73\u53F0\u8FB9\u7F18\u7684\u6728\u7BB1\u4E0A\u4E00\u8DC3\u800C\u4E0B\u3002\u91D1\u53D1\u5C11\u5973\u8E29\u7740\u732B\u6B65\uFF0C\u6765\u5230\u6211\u7684\u9762\u524D\u3002\r"
    isPlayer: 0
  - content: "\u6742\u9C7C\u4EBA\u7C7B\uFF0C\u4E0D\u53EA\u662F\u65E0\u80FD\uFF0C\u66F4\u662F\u65E0\u793C\u5462\uFF0C\u8981\u4E0D\u8FD8\u662F\u628A\u72D7\u72D7\u653E\u8FDB\u6765\u597D\u4E86\u2026\u2026\r\n\u8BF4\u7F62\uFF0C\u5C11\u5973\u62AC\u8D77\u624B\u81C2\u3002\u201C\u730E\u72AC\u201D\u8EAB\u524D\u7684\u7A7A\u95F4\u5982\u540C\u73BB\u7483\u7FFB\u52A8\u6298\u53E0\uFF0C\u4F34\u968F\u8FD9\u5F02\u6837\u53D8\u5316\u7684\uFF0C\u8FD8\u6709\u201C\u730E\u72AC\u201D\u66F4\u4E3A\u7126\u8E81\u4E0E\u5174\u594B\u7684\u4F4E\u543C\u3002\r"
    isPlayer: 0
  - content: "\u7B28\u86CB~\u6742\u9C7C~\u8BF4\u53E5\u611F\u8C22\u7684\u8BDD\u5C31\u653E\u8FC7\u4F60\u54E6~"
    isPlayer: 0
  - content: "(\u865A\u5F31)\u8C22\u2026\u2026\u591A\u8C22\u2026\u2026?"
    isPlayer: 1
  - content: "\u4F60\u2026\u2026\u7B97\u5566\uFF0C\u53CD\u6B63\u4E5F\u6CA1\u6307\u671B\u4F60\u8FD9\u6837\u7684\u866B\u5B50\u80FD\u660E\u767D\uFF0C\u603B\u4E4B\u90A3\u4E9B\u5BB6\u4F19\u8981\u6765\u4E86\u3002"
    isPlayer: 0
  - content: "\u4F60\u5728\u8BF4\u4EC0\u2026\u2026"
    isPlayer: 1
  - content: "\u6025\u4FC3\u7684\u811A\u6B65\u58F0\uFF0C\u4F34\u7740\u67D0\u79CD\u6606\u866B\u632F\u7FC5\u7684\u566A\u58F0\u4ECE\u821E\u53F0\u7684\u56DB\u9762\u516B\u65B9\u54CD\u8D77"
    isPlayer: 1
  - content: "\u7B49\u7B49\uFF0C\u8FD9\u662F\u600E\u4E48\u56DE\u4E8B\uFF01"
    isPlayer: 1
  - content: "\u8981\u4F60\u547D\u7684\u6765\u5566~\u9700\u8981\u672C\u5C0A\u518D\u6551\u4F60\u4E00\u6B21\u5417\u2026\u2026\u662F\u4E0E\u5426\u53EF\u7531\u4E0D\u5F97\u4F60\u54E6"
    isPlayer: 0
  - content: "\u624B\u8155\u7684\u707C\u75DB\u5F88\u5FEB\u8F6C\u79FB\u4E86\u6211\u5BF9\u5979\u90A3\u8DF3\u8131\u8BDD\u8BED\u7684\u6CE8\u610F\u529B\uFF0C\u4E00\u4E2A\u9EC4\u8272\u7684\u626D\u66F2\u5370\u8BB0\u4E0D\u77E5\u4F55\u65F6\u70D9\u5728\u4E86\u6211\u7684\u5DE6\u624B\u8155\u90E8\uFF0C\u4E00\u4EF6\u51ED\u7A7A\u51FA\u73B0\u7684\u6C89\u91CD\u98CE\u8863\u7A81\u7136\u8986\u76D6\u4E86\u6211\u7684\u8EAB\u4F53\u3002"
    isPlayer: 1
  - content: "\u554A\u5566~\u8FD8\u633A\u597D\u770B\u7684\u561B~\u770B\u770B\u53E3\u888B\u91CC\u6211\u7ED9\u4F60\u7684\u793C\u7269\uFF1F"
    isPlayer: 0
